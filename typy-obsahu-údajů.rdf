<rdf:Description rdf:about="https://data.dia.gov.cz/zdroj/číselníky/typy-obsahu-údajů">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
	<skos:prefLabel xml:lang="cs">Typy obsahu údajů v ISVS dle § 23, odst. 3, vyhlášky č. 360/2023 Sb.</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="https://data.dia.gov.cz/zdroj/číselníky/typy-obsahu-údajů/položky/identifikační">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="https://data.dia.gov.cz/zdroj/číselníky/typy-obsahu-údajů"/>
	<skos:prefLabel xml:lang="cs">Identifikační</skos:prefLabel>
	<skos:notation>identifikační</skos:notation>
	<skos:definition xml:lang="cs">Údaje, které, které samostatně nebo ve skupině identifikují konkrétní objekt či subjekt práva</skos:definition>
</rdf:Description>

<rdf:Description rdf:about="https://data.dia.gov.cz/zdroj/číselníky/typy-obsahu-údajů/položky/evidenční">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="https://data.dia.gov.cz/zdroj/číselníky/typy-obsahu-údajů"/>
	<skos:prefLabel xml:lang="cs">Evidenční</skos:prefLabel>
	<skos:notation>evidenční</skos:notation>
	<skos:definition xml:lang="cs">Údaje, které neidentifikují objekt či subjekt práva, ale váží se k němu v rámci agendy a evidence, která se v ní vede</skos:definition>
</rdf:Description>

<rdf:Description rdf:about="https://data.dia.gov.cz/zdroj/číselníky/typy-obsahu-údajů/položky/statistické">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="https://data.dia.gov.cz/zdroj/číselníky/typy-obsahu-údajů"/>
	<skos:prefLabel xml:lang="cs">Statistické</skos:prefLabel>
	<skos:notation>statistické</skos:notation>
	<skos:definition xml:lang="cs">Údaje, které seskupují a formou anonymizace či pseudonymizace zamezují vazbě na objekt či subjekt práva</skos:definition>
</rdf:Description>

